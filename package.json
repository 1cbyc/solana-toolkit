    "@rollup/plugin-node-resolve": "^15.2.3",
    "@rollup/plugin-terser": "^0.4.4",
    "eslint": "^9.0.0",
    "jest": "^29.7.0",
    "jsdoc": "^4.0.2",
    "rollup": "^4.12.0"
  },
  "engines": {
    "node": ">=16.0.0"
