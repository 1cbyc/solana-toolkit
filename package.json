  "dependencies": {
    "@solana/spl-token": "^0.4.0",
    "@solana/web3.js": "^1.91.8",
    "bs58": "^5.0.0",
    "solana-toolkit": "^0.0.7",
    "tweetnacl": "^1.0.3",
    "tweetnacl-util": "^0.15.1"
